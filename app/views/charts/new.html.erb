<h1>New chart</h1>

<%= form_for(@chart) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>
  <p>
		<%= f.label :chart_type %> <%= f.select :chart_type, Chart::CHART_TYPES %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', charts_path %>